{
  "db": {
    "url": "postgresql://postgres:postgres@/covid_chance_2"
  },
  "feeds": [
    {
      "name": "Example Feed",
      "url": "https://www.example.com/rss.xml"
    }
  ],
  "download_feeds": {
    "timeout": 30
  },
  "download_pages": {
    "since": "2020-03-01",
    "wait_min": 0,
    "wait_max": 0,
    "timeout": 10
  },
  "match_lines": {
    "keyword_lists": [
      ["coronavirus", "covid"],
      ["chance", "opportunity"]
    ]
  },
  "parse_lines": {
    "pattern": "(?P<parsed>.+)"
  },
  "post_tweet": {
    "profile_name": "Covid-19 is...",
    "profile_description_template": "${n_posted}/${n_total}",
    "tweet_template": "${text} ${url}"
  },
  "print_export": {
    "context": {
      "name": "Covid-19 is...",
      "handle": "@covid_chance"
    },
    "tweets_per_page": 12,
    "output_filename_template": "covid_chance-$i.html",
    "template_package": ["covid_chance", "templates", "print.html"],
    "default_tz": "Europe/Berlin"
  }
}
